{
  "name": "redux-universal-starter",
  "version": "0.1.0",
  "description": "Simple babel boilerplate with crucial and up-to-date dependencies",
  "scripts": {
    "build": "babel src -d dist",
    "test": "standard && mocha --recursive --compilers js:babel-register",
    "lint": "standard",
    "dev": "nodemon --exec npm run lint"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.4",
    "standard": "^7.1.2",
    "webpack": "^1.13.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "koa": "^1.2.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2"
  }
}
